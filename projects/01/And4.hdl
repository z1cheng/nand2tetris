/**
 * 4-bit bitwise And:
 * for i = 0..3: out[i] = (a[i] and b[i])
 */

CHIP And4 {
    IN a[4], b[4];
    OUT out[4];

    PARTS:
    // Put your code here:
    And(a=a[0], b=b[0], out=out[0]);
    And(a=a[1], b=b[1], out=out[1]);
    And(a=a[2], b=b[2], out=out[2]);
    And(a=a[3], b=b[3], out=out[3]);
}